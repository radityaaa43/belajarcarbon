<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bcoe.bricarbon.dao.ContractTemplateDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.bcoe.bricarbon.entity.ContractTemplateEntity" id="contractTemplateMap">
        <result property="id" column="id"/>
        <result property="contractName" column="contract_name"/>
        <result property="contractAbi" column="contract_abi"/>
        <result property="contractBin" column="contract_bin"/>
        <result property="contractBase64" column="contract_base64"/>
    </resultMap>
    
    <select id="queryByTemplate" parameterType="java.lang.String" resultType="com.bcoe.bricarbon.entity.ContractTemplateEntity">
    	select * from contract_template where contract_name = #{template}
    </select>


</mapper>